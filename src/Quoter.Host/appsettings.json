{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File",
      "Serilog.Sinks.Console"
    ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./Logs/Quoter.log",
          "rollingInterval": "Day"
        }
      },
      { "Name": "Console" }
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    }
  },
  "AllowedHosts": "*"
}
